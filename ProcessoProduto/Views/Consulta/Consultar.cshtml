@model List<ProcessoProduto.Models.ProdutoView>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<div class="container-fluid">
<nav class="navbar navbar-expand-lg navbar-light bg-light">
  <a class="navbar-brand" href="#">Navbar</a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>

  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
      <li class="nav-item active">
        <a class="nav-link" href="@Url.Action("Inicio", "Inicial")">Home <span class="sr-only">(current)</span></a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="@Url.Action("Cadastrar", "Cadastro")">Cadastro</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="@Url.Action("Consultar", "Consulta")">Consulta</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="@Url.Action("Pesquisar", "Pesquisa")">Pesquisa</a>
      </li>
    </ul>
    <form class="form-inline my-2 my-lg-0">
      <input class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search">
      <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
    </form>
  </div>
</nav>
<hr/>
<div class="modal-dialog">
    <div class="modal-content">
              <div class="modal-header">
                <h4 class="modal-title">Consultar a passagem</h4>
            </div>
        </div> 
   </div>
   <table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">username</th>
      <th scope="col">Fullname</th>
      <th scope="col">Date</th>
       <th scope="col">Country</th>
      <th scope="col">Active</th>
    </tr>
  </thead>
  <tbody>
      @foreach(var produto in Model)
      {
             <td>{produto.Id}</td>
             <td>{produto.Fullname}</td>
             <td>{produto.Date}</td>
             <td>{produto.Country}</td>
             <td>{produto.Active}</td>
             <td id="Deletar?id">Excluir</td>
             <td id="Atuaslizar?id">Alterar</td>
      }
        </tbody>
</table>
</div>